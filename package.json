{
  "name": "wym-eslint-vue",
  "version": "1.0.0",
  "description": "vue 项目的eslint安装和文件配置",
  "main": "index.js",
  "scripts": {
    "build:pre": "vue-tsc --noEmit && vite build --mode staging",
    "build:pro": "vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint \"src/**/*.{js,vue,ts}\" --fix",
    "lint:css": "stylelint src/**/*.{vue,css,sass,scss,less} --fix",
    "prettier-format": "prettier --config .prettierrc.cjs \"src/**/*.{js,vue,ts}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangyaming-08/wym-eslint-vue.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wangyaming-08/wym-eslint-vue/issues"
  },
  "homepage": "https://github.com/wangyaming-08/wym-eslint-vue#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/eslint": "^8.40.2",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "stylelint": "^15.10.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite-plugin-stylelint": "^4.3.0"
  }
}
